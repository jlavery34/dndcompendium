{
    "manifest_version": 3,
    "name": "DnD Spell Search",
    "key": "manifest@v3.com",
    "version": "1.0",
    "permissions": ["activeTab"],
    "host_permissions": ["https://www.dnd5eapi.co/"],
    "action": {
      "default_popup": "src/popup.html",
      "default_icon": {
        "128": "icons/128d20.jpg"
      }
    },
    "background": {
      "scripts": ["src/background.js"]
    },  
    "icons": {
      "128": "icons/128d20.jpg"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "manifest@v3.com"
      }
    }
    
  }
  